{
  "name": "toille-vue",
  "version": "0.9.0",
  "description": "A test website for Elliot Dickerson",
  "scripts": {
    "start": "astro dev",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "pages:dev": "mv wrangler.toml wrangler.toml.bak && wrangler pages dev --d1 DB=toille-vue-db -- npm run dev; mv wrangler.toml.bak wrangler.toml",
    "db:init": "wrangler d1 execute toille-vue-db --local --file=./schema.sql",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.57.0",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-vue": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.21.0",
    "flush-promises": "^1.0.2",
    "globals": "^15.15.0",
    "jsdom": "^27.0.0",
    "prettier": "3.5.3",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.4.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/vue": "^5.1.4",
    "@nanostores/vue": "^1.0.1",
    "@picocss/pico": "^2",
    "astro": "^5.16.12",
    "nanostores": "^1.1.0",
    "three": "^0.181.2",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  }
}