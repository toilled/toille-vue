{
  "name": "toille-vue",
  "version": "0.9.0",
  "description": "A test website for Elliot Dickerson",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build --outDir dist",
    "build:server": "vite build --config vite.config.ssr.ts --ssr && find functions -maxdepth 1 -type f \\( -name '*.png' -o -name '*.svg' -o -name '*.ico' -o -name '*.xml' -o -name '*.txt' -o -name '*.webmanifest' \\) -delete",
    "serve": "vite preview",
    "pages:dev": "mv wrangler.toml wrangler.toml.bak && wrangler pages dev --d1 DB=toille-vue-db -- bun run dev; mv wrangler.toml.bak wrangler.toml",
    "db:init": "wrangler d1 execute toille-vue-db --local --file=./schema.sql",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@playwright/test": "^1.57.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@testing-library/svelte": "^5.3.1",
    "@tsconfig/svelte": "^5.0.7",
    "@types/three": "^0.181.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.21.0",
    "flush-promises": "^1.0.2",
    "globals": "^15.15.0",
    "jsdom": "^27.0.0",
    "prettier": "3.5.3",
    "svelte": "^5.51.2",
    "svelte-check": "^4.4.0",
    "svelte-preprocess": "^6.0.3",
    "terser": "^5.44.1",
    "tinro": "^0.6.12",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.4.1",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^3.2.4",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "@picocss/pico": "^2",
    "three": "^0.181.2"
  }
}